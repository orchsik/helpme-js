{
  "name": "helpme-js",
  "version": "0.1.5",
  "description": "A js utilities package for node.js for me",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node lib/index.ts",
    "start:B": "babel-node lib/index.ts",
    "build": "npm run build:typescript && npm run build:babel",
    "build:B": "npm run build:typescript",
    "build:typescript": "tsc",
    "build:babel": "babel lib --out-dir dist --extensions \".js,.jsx,.ts,.tsx\"",
    "prebuild": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "watch": "jest -o --watch"
  },
  "keywords": [
    "javascript",
    "util"
  ],
  "author": "orchsik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/orchsik/helpme-js"
  },
  "bugs": {
    "url": "https://github.com/orchsik/helpme-js/issues",
    "email": "orchsik@naver.com"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
